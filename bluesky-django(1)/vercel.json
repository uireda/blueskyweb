{
  "version": 2,
  "builds": [
    {
      "src": "bluesky_tourism/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "bluesky_tourism/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "bluesky_tourism.settings"
  }
}